{
    "env": {
        "development": {
            "presets": [
                [
                    "env", {
                        "es2015": {
                            "modules": false
                        },
                        "targets": {
                            "ie": 9,
                            "uglify": true
                        }
                    }
                ],
                "react",
                "stage-1"
            ],
            "plugins": [
                "transform-react-constant-elements",
                "transform-react-jsx",
                "transform-react-remove-prop-types"
            ]
        },
        "production": {
            "presets": [
                [
                    "env", {
                        "es2015": {
                            "modules": false
                        },
                        "targets": {
                            "ie": 9,
                            "uglify": true
                        }
                    }
                ],
                "react",
                "stage-1"
            ],
            "plugins": [
                "transform-react-constant-elements",
                "transform-react-jsx",
                "transform-react-remove-prop-types"
            ]
        },
        "test": {
            "presets": [
                "env",
                "react",
                "stage-1"
            ]
        }
    }
}
